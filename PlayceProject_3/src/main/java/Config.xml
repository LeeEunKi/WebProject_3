				<property name="username" value="hr"/>
				<property name="password" value="happy"/>
			</dataSource>
		</environment>
	</environments>
	<mappers>
	<!-- 지윤/ Mappers는 아직 등록 안했습니다 -->
	</mappers>
</configuration>